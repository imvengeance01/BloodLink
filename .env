# Server
PORT=3001
NODE_ENV=development

# Database (PostgreSQL)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DATABASE?schema=public"

# JWT
JWT_SECRET=your-super-secret-key-min-32-chars
